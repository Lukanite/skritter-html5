{
    "manifest_version": 2,
    "name": "Skritter",
    "description": "Learn to Write Chinese and Japanese Characters",
    "version": "0.0.1",
    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "icons": {
        "16": "icon-16x16.png",
        "128": "icon-128x128.png",
        "196": "icon-196x196.png"
    },
    "offline_enabled": true,
    "permissions": [
        "https://beta.skritter.cn/api/*",
        "https://beta.skritter.com/api/*"
    ]
}